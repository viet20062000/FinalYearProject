#:import os os
<CinebotAssistantScreen>:
	MDBoxLayout:
		orientation: 'vertical'

		MDFillRoundFlatButton:
			id:controller
			text:"Start"
			pos_hint: {'center_x': .5}
			on_release:root.start_assistant()
		MDBoxLayout:
			id:assistant_conversation
			orientation: 'vertical'

			ScrollView:
				id:speech_scroll_view
				MDList:
					spacing: "10dp"
					padding:"5dp"
					id:all_conversations
